@model IEnumerable<PiDevCRM.Domain.Entities.Resources>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdProspection)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeResource)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Availability)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdProspection)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeResource)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Availability)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdResource }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdResource }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdResource })
        </td>
    </tr>
}

</table>
